pico-8 cartridge // http://www.pico-8.com
version 34
__lua__

function _init()
  x = 60
  y = 60
  s = 1
end

function _update()
  s = 1
  
  if btn(1) then
    x = x + 1  
    s = 1
  elseif btn(0) then
    x = x - 1
    s = 2
  end
  
  if btn(2) then
    y = y - 1
    s = 4
  elseif btn(3) then
    y = y + 1
    s = 3
  end
  
end

function _draw()
  cls()
  spr(s, x, y)
end
