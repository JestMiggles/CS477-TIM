function _init()
  init_player()
  mx = 0
  my = 0
 
  cx = 0
  cy = 0
  map_x = 0
  hill_x = 0
  mnt_x = 0
  map_spd = 1
  hill_spd = 0.5
  mnt_spd = 0.25
end

function _update()
  move_player()
  map_x -= map_spd
  if map_x<- 127 then map_x = 0 end
  hill_x -= hill_spd
  if hill_x<- 127 then hill_x = 0 end
  mnt_x -= mnt_spd
  if mnt_x<- 127 then mnt_x = 0 end
  
  if x > 127 then
    cx = 128
  else 
    cx = 0
  end
  
end

function _draw()
  cls(12)
  camera(cx,cy)
 -- map(mx,my)
  map(32,0,mnt_x,0,16,16)
  map(32,0,mnt_x+128,0,16,16)
  map(16,0,hill_x,0,16,16)
  map(16,0,hill_x+128,0,16,16)
  map(0,0,map_x,0,16,16)
  map(0,0,map_x+128,0,16,16)
  draw_player()
 
end
