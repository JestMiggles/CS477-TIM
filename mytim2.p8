-- main
function _init()  
  ts = 0  
  state = "start"
  init_game() 
  init_pickups()
end

function _update() 
  if state == "start" then
    update_start()
  elseif state == "game" then
    update_game()
    updateparts()
    update_pickups()
  elseif state == "over" then
    game_over()
  end 
  
end

function _draw() 
  if state == "start" then
    draw_start()
  elseif state == "game" then
    draw_map()
    draw_player()
    draw_pickups()

    print(rooms[5].x,36,2,8)
    print(rooms[5].y,36,6,9)

    print(cx,2,2,8)
    print(cy,6,6,9)

  end 
  
  if false then
    rect(box.x,box.y,
         box.x2,box.y2,8)
  end 
  
  if pl.delay > 0 then
    print('cd:'..pl.delay/30,2,2,8)
  end
   
end

