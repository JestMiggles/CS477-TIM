function addmob(typ,mobx,moby)
  local m = {
    x = mobx,
    y = moby,
    hp = mob_hp[typ],
    homax = mob_hp[typ],
    atk = mob_atk[typ],
    ani = {17,18,19,20}
  }  
  local s = {
    x = mobx,
    y = moby,
    ani = {33,34,35,36}
  }
  if typ == 0 then  
    add(mob,m) 
  else
    add(mob,s) 
  end
end

function getframe(ani)
  return ani[flr(t/8)%4+1]
end

function drawspr(_spr,_x,_y,_c)   
   camera(cx,cy)
   spr(_spr,_x,_y)
   camera(0,0)
end

function mobwalk(mob,dx,dy)
  mob.x += dx
  mob.y += dy

end

