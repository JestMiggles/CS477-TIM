pico-8 cartridge // http://www.pico-8.com
version 34
__lua__
-- main
function _init()
  state = update_game
  init_player()    
end

function _update()
  state()   
end

function _draw()  
  draw_map()
  draw_player()
  
  if true then
    rect(box.x,box.y,
         box.x2,box.y2,8)
  end  
end

-->8
-- movement and updates
function init_player()
  pl = {}
  pl.x = 60
  pl.y = 60
  pl.ox = 60
  pl.oy = 60
  pl.w = 8
  pl.h = 8
  pl.s = 1
  pl.tle = 1
  
  cx = 0
  cy = 0
  
  box = {}
  box.x = 32
  box.y = 32
  box.x2 = 88
  box.y2 = 88
end

function update_game()
  pl.s = 3
  pl.ox = pl.x
  pl.oy = pl.y
  pl.tle = mget(pl.x,pl.y)
  
  --movement
  if btn(➡️) then
    pl.x += 2  
    pl.s = 1
    if btn(❎) then
      pl.x += 4
    end    
  elseif btn(⬅️) then
    pl.x -= 2
    pl.s = 2
    if btn(❎) then
      pl.x -= 4
    end
  end
  
  if btn(⬆️) then
    pl.y -= 2
    pl.s = 4
    if btn(❎) then
      pl.y -= 4
    end    
  elseif btn(⬇️) then
    pl.y += 2
    pl.s = 3
    if btn(❎) then
      pl.y += 4
    end     
  end 
  
  --collision detection
  if map_collide(pl.x,pl.y,
                 pl.w,pl.h) then
     if fget(pl.tle,2) then
        item_collide(pl.tle,pl.x,pl.y)
     end
     bump()
  end 
  
  --camera movement
  if pl.x < box.x then
    pl.x = box.x
    if btn(❎) then
      cx -= 6
    end
    cx -= 2
  elseif pl.x > box.x2 then
    pl.x = box.x2
    if btn(❎) then
      cx += 6
    end
    cx += 2
  end
  
  if cx <= 0 then
    cx = 0
    box.x = 0
  elseif cx >= 896 then
    cx = 896    
    box.x2 = 127
  else 
    box.x = 32
    box.x2 = 88 
  end
  
  if pl.y < box.y then
    pl.y = box.y
    if btn(❎) then
      cy -= 6
    end
    cy -= 2
  elseif pl.y > box.y2 then
    pl.y = box.y2
    if btn(❎) then 
      cy += 6
    end
    cy += 2
  end
  
  if cy <= 0 then
    cy = 0
    box.y = 0
  elseif cy >= 896 then
    cy = 896
    box.y2 = 127
  else 
    box.y = 32
    box.y2 = 88
  end

end

function game_over()

end


-->8
--collision and draw
function draw_map()
  cls()
  camera(cx,cy)
  map(mx,my)
  camera(0,0)
  
end

function draw_player()
  spr(pl.s, pl.x, pl.y)
end

function bump()  
  if pl.x > pl.ox then
    pl.x = pl.ox - 0.9999
  elseif pl.x < pl.ox then
    pl.x = pl.ox + 0.9999
  end 
     
  if pl.y > pl.oy then
    pl.y = pl.oy - 0.9999
  elseif pl.y < pl.oy then
    pl.y = pl.oy + 0.9999
  end  
end

function map_collide(x,y,
                     w,h)
  x += cx 
  y += cy
  
  s1 = mget(x / 8, y / 8)
  s2 = mget((x+w-1) /8, y / 8)            
  s3 = mget(x / 8,(y+w-1) /8)
  s4 = mget((x+w-1) /8,(y+w-1) /8)
            
  if fget(s1,3) then                        
    return true
  elseif fget(s2,3) then
    return true
  elseif fget(s3,3) then
    return true
  elseif fget(s4,3) then
    return true
  end
  
  return false
end

function item_collide(tle,x,y)
  if tle == 23 or tle == 24 then
    --health
    mset(pl.x,pl.y,8)
  elseif tle == 39 or tle == 40 then
    --damageboost
    mset(pl.x,pl.y,8)
  end
  
end
